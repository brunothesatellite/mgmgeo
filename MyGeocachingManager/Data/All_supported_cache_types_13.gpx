        </groundspeak:log>
        <groundspeak:log id="585926988">
          <groundspeak:date>2016-03-28T19:00:00Z</groundspeak:date>
          <groundspeak:type>Webcam Photo Taken</groundspeak:type>
          <groundspeak:finder id="12580612">HHT &amp; TS</groundspeak:finder>
